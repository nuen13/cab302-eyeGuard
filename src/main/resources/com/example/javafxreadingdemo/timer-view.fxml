<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafxreadingdemo.TimerManagementApplication"
      alignment="CENTER">

    <!-- Title Label -->
    <Label text="eyeGuard" style="-fx-font-size: 18pt;" />
    <!-- Spacer -->
    <Label text="" />

    <!-- Other UI elements -->
    <Label fx:id="timerLabel"/>
    <Button text="Start" onAction="#onStartButtonClicked"/>
    <Button text="Pause" onAction="#onPauseButtonClicked"/>
    <Button text="Reset" onAction="#onResetButtonClicked"/>
    <Button text="Analytics" onAction="#onAnalyticsButtonClicked"/>
    <TextField fx:id="breakIntervalField" promptText="Enter break interval in seconds"/>
    <Button text="Set Break Interval" onAction="#onSetBreakIntervalButtonClicked"/>
</VBox>
